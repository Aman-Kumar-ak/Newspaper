# Frontend .dockerignore

# Dependencies (will be installed in container)
node_modules/
npm-debug.log
yarn-debug.log
yarn-error.log

# Build output (will be built in container)
dist/
dist-ssr/
build/

# Environment files (use build args)
.env
.env.*
!.env.example

# Development files
.vite/
.cache/

# Documentation
*.md

# Git
.git/
.gitignore

# IDE
.vscode/
.idea/
*.swp

# Tests
test/
tests/
*.test.js
*.spec.js
coverage/

# OS files
.DS_Store
Thumbs.db

# Temporary
*.tmp
.tmp/

# Config files not needed
.prettierrc
.eslintrc
eslint.config.js
